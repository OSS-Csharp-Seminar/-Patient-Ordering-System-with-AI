@using N_Tier.Core.Entities;
@page
@model Patient

<h1>Patient Login</h1>

<form method="post">
    <div>
        <label for="name">Name:</label>
        <input type="text" id="name" name="Name" />
    </div>
    <div>
        <label for="password">Password:</label>
        <input type="password" id="password" name="Password" />
    </div>
    <button type="submit">Login</button>
</form>
